<div>
  <div class="container">
    <h1 class="users_heading">ユーザー一覧</h1>
    <div class="users_wrapper">
      <% @users.each do |user| %>
        <div class="user">
          <img class="user_icon" src="<%= "/user_images/#{user.image_name}" %>">
          <div class="user_name">
            <%= link_to(user.name, "/users/#{user.id}") %>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
